
#include <stdio.h>
int main() {
 int n;
 printf("Enter a positive integer: ");
 scanf("%d", &n);
 if (n < 1) {
 printf("Error: Please enter a positive integer.\n");
 return 1;
 }
 printf("Numbers from 1 to %d:\n", n);
 for (int i = 1; i <= n; i++) {
 printf("%d ", i);
 }
 printf("\n");
 return 0;
}
